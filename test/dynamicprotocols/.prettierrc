{
	"semi": true,
	"singleQuote": false,
	"trailingComma": "none",
	"arrowParens": "always",
	"printWidth": 88,
	"tabWidth": 2,
	"useTabs": true,
	"endOfLine": "auto",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"overrides": [
		{
			"files": ["*.css", "*.scss", "*.less"],
			"options": {
				"singleQuote": false
			}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"options": {
				"trailingComma": "es5"
			}
		}
	]
}
