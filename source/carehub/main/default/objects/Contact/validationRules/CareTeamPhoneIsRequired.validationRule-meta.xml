<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CareTeamPhoneIsRequired</fullName>
    <active>false</active>
    <errorConditionFormula>AND(RecordType.DeveloperName = &apos;CareCircle&apos;, ISBLANK( Phone ))</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>Ensure the phone number of the care circle is filled in.</errorMessage>
</ValidationRule>
